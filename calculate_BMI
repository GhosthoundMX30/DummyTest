from dummy import greeting

def BMI(height, weight):
    return weight / (height/100)**2

if __name__ == "__main__":
    name = input("Dame tu nombre: ")
    w = float(input("Dame tu peso (kg): "))
    h = float(input("Dame tu altura (cm): "))
    print('\n', greeting(name))
    print(f'\nTu IMC es: {round(BMI(h, w), 1)}')